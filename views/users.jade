extends layout

block content
  script(id='userTemplate' type = 'text/x-handlebars-template').
    {{#each this}}
        <div class="new user">
            <p>Name {{name}}</p>
            <p>Name {{age}}</p>
            <p>Name {{gender}}</p>
            <p>Name {{location}}</p>
            <p>Name {{twitterHandle}}</p>
            <button id={{id}}> Delete </button>
        </div>
    {{/each}}

